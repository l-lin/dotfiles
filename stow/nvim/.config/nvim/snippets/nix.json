{
  "direnv": {
    "prefix": "direnv",
    "body": [
      "{ pkgs ? import <nixpkgs> {}}:",
      "",
      "pkgs.mkShell {",
      "  packages = [ pkgs.${1} ];",
      "}"
    ],
    "description": "Create new direnv."
  },
  "home": {
    "prefix": "home",
    "body": [
      "#",
      "# ${1}",
      "# src: ${2}",
      "#",
      "",
      "{ pkgs, ... }: {",
      "  home.packages = with pkgs; [ ${4} ];",
      "}"
    ],
    "description": "Create new home-manager module."
  },
  "mise": {
    "prefix": "mise",
    "body": [
      "xdg.configFile.\"mise/conf.d/${1}.toml\".source = ./.config/mise/conf.d/${1}.toml;"
    ],
    "description": "Add mise tool configuration file."
  }
}
