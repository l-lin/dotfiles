# All keybindings viewable here: https://github.com/ncmpcpp/ncmpcpp/blob/master/doc/bindings

def_key "?"
  show_help

def_key "j"
  scroll_down
def_key "k"
  scroll_up

def_key "ctrl-u"
  page_up
def_key "ctrl-d"
  page_down

def_key "P"
  show_playlist
def_key "v"
  show_visualizer

def_key "R"
  toggle_random
