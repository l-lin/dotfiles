{
  "lastChangelogVersion": "0.54.0",
  "theme": "light",
  "defaultProvider": "github-copilot",
  "defaultModel": "gpt-4o",
  "skills": [
    "~/.config/ai/skills"
  ],
  "prompts": [
    "~/.config/ai/commands"
