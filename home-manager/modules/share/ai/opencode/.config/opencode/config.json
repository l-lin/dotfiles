{
  "$schema": "https://opencode.ai/config.json",
  "keybinds": {
    "command_list": "alt+k",
    "input_clear": "ctrl+c,ctrl+u",
    "input_newline": "enter,ctrl+j",
    "input_submit": "ctrl+s",
    "history_previous": "ctrl+p",
    "history_next": "ctrl+n",
    "messages_copy": "<leader>y,alt+y"
  },
  "autoshare": false,
  "share": "disabled",
  "autoupdate": false,
  "instructions": [],
  "layout": "stretch"
}
