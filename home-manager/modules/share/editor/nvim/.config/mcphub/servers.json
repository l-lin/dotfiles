{
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true
    },
    "neovim": {
      "disabled_tools": []
    }
  },
  "mcpServers": {
    "memory": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "-v",
        "agent-memory:/app/dist",
        "--rm",
        "mcp/memory"
      ]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ]
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    }
  }
}
