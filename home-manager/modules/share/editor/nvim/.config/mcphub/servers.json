{
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true
    },
    "neovim": {
      "disabled_tools": []
    }
  },
  "mcpServers": {
    "sequentialthinking": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/sequentialthinking"]
    },
    "atlassian": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.atlassian.com/v1/sse"]
    }
  }
}
